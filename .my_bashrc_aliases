# aldev vm aliases
alias aldev-connect="VBoxManage startvm "aldev" --type=headless ; ssh aldev"
alias aldev-shutdown="VBoxManage controlvm "aldev" acpipowerbutton"


# cisco anyconnect cli aliases
alias alvpn-disconnect="/opt/cisco/anyconnect/bin/vpn disconnect"
alias alvpn-status="/opt/cisco/anyconnect/bin/vpn status"


# kpcli aliases
alias kp="kpcli --kdb '$HOME/GDrive/private/PasswordDatabase.kdbx' --pwfile '$HOME/.kpmp'"


# emacs aliases
alias ec='emacsclient -nw "$@"'
